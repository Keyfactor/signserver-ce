<!-- 
    Sample of a local Maven settings file overriding the default URLs of Maven
    Central to use the Keyfactor's internal repository.
    
    Community/non-Keyfactor: Instead use sample-maven-settings-community.xml
    
    Copy to ~/.m2/settings.xml or edit your existing file if you have one.
    
    The current settings can be seen by running:
    $ mvn help:effective-settings
-->

<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                          http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <servers>
        <server>
            <username>firstname.lastname@keyfactor.com</username>
            <password>your access token</password>
            <id>keyfactor</id>
        </server>
    </servers>
    <mirrors>
        <mirror>
            <id>keyfactor</id>
            <name>Keyfactor</name>
            <mirrorOf>*</mirrorOf>
            <url>https://keyfactor.jfrog.io/artifactory/dev-maven</url>
        </mirror>
    </mirrors>
</settings>
