<?xml version="1.0" encoding="UTF-8"?>
<project name="SignServer" xmlns="http://maven.apache.org/DECORATION/1.6.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.6.0 http://maven.apache.org/xsd/decoration-1.6.0.xsd">
    <bannerLeft>
        <name>SignServer - Open Source server side Digital Signatures</name>
        <src>images/SignServer_pki_by_primekey.png</src>
        <href>index.html</href>
    </bannerLeft>
    <publishDate position="none"/>
    <version/>
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.5</version>
    </skin>
    <poweredBy>
        <logo name="EJBCA - PKI by PrimeKey" img="images/ejbca_pki_by_primekey.png" href="https://www.primekey.se/technologies/products-overview/ejbca-enterprise/" alt="Click the PKI-by-PrimeKey-EJBCA-logo to visit www.ejbca.org" />
        <logo name="PrimeKey Solutions" img="images/logo-PrimeKey.png" href="https://www.primekey.se" alt="Click the PrimeKey logo to check out the products and services offered at www.primekey.se" />
    </poweredBy>
    
    <!--  <bannerRight>
      <src>http://maven.apache.org/images/maven-small.gif</src>
    </bannerRight>-->

    <body>
        <links>
            <item name="PrimeKey Solutions"    href="https://www.primekey.se/" />
            <item name="EJBCA"                 href="https://www.ejbca.org/"/>
            <item name="SignServer.org"        href="https://www.signserver.org"/>
        </links>

        <menu name="Documentation">
            <item name="Home"                  href="/index.html"/>

            <item name="Architecture"          href="/architecture.html"/>

            <item name="Manual Overview"       href="/manual.html"/>
        </menu>

        <menu name="Manual">    
            <item name="Introduction"          href="/manual/introduction.html"/>
            <item name="Installation Guide"    href="/manual/installguide.html"/>        
            <item name="Usage Guide"           href="/manual/usageguide.html"/>
            <item name="Integration"           href="/manual/integration.html"/>
            <item name="Common Configuration"  href="/manual/commonconf.html"/>
            <item name="Plugins"               href="/manual/plugins.html"/>
            <item name="Development"           href="/manual/development.html"/>
            <item name="Miscellaneous"         href="/manual/misc.html"/>
        </menu>

        <menu name="Other">
            <item name="signserver.org"        href="https://www.signserver.org"/>
        </menu>
    </body>
    
    <!-- Remove date in header as it breaks reproducible builds. -->
    <custom>
        <fluidoSkin>
            <skipGenerationDate>true</skipGenerationDate>
            
        </fluidoSkin>
    </custom>
</project>
