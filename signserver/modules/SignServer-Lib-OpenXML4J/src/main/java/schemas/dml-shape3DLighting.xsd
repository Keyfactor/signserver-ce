      </xsd:enumeration>
      <xsd:enumeration value="harsh">
        <xsd:annotation>
          <xsd:documentation>Harsh</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="flood">
