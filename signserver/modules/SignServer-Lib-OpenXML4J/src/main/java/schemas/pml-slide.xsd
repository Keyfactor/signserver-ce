          <xsd:documentation>Slide Layout Type Enumeration ( Text and Chart )</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="chartAndTx">
        <xsd:annotation>
          <xsd:documentation>Slide Layout Type Enumeration ( Chart and Text )</xsd:documentation>
        </xsd:annotation>
