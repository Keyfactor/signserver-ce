  <xsd:simpleType name="ST_PlaceholderSize">
    <xsd:annotation>
      <xsd:documentation>Placeholder Size</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="full">
