        <plugins>
            <!-- Run ant deploy from Maven (or the IDE) using: mvn -Pprofile-empty antrun:run. -->
            <!-- Note: This requers a profile without sub modules otherwise the Ant target is run in each sub module. -->
            <!-- Note: This is mapped to Run Project in NetBeans IDE (nbactions.xml). -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-antrun-plugin</artifactId>
                <configuration>
                    <target>
                        <ant target="deploy"/>
                    </target>
                </configuration>
