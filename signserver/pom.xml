    <properties>
        <!-- fixedTime / dist.fixedTime is used for deterministic builds. -->
        <fixedTime/>
        <dist.fixedTime>${fixedTime}</dist.fixedTime>

        <!-- Bouncy Castle artifacts name, so we can easily switch to the debug ones -->
        <bouncycastle.bcpg.artifact>bcpg-jdk18on</bouncycastle.bcpg.artifact>
        <bouncycastle.bcpkix.artifact>bcpkix-jdk18on</bouncycastle.bcpkix.artifact>
        <bouncycastle.bcprov.artifact>bcprov-jdk18on</bouncycastle.bcprov.artifact>
