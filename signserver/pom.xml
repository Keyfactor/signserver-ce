        <!-- Bouncy Castle artifacts version -->
        <bouncycastle.bcpg.artifact.version>1.78</bouncycastle.bcpg.artifact.version>
        <bouncycastle.bcpkix.artifact.version>1.78</bouncycastle.bcpkix.artifact.version>
        <bouncycastle.bcprov.artifact.version>1.78</bouncycastle.bcprov.artifact.version>
        <bouncycastle.bcutil.artifact.version>1.78</bouncycastle.bcutil.artifact.version>

        <!-- Platform independent build -->
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>

        <dss.framework.version>5.9</dss.framework.version>
        <rest-assured.version>5.3.0</rest-assured.version>
    </properties>

    <profiles>
        <!-- Default profile. -->
        <profile>
            <id>profile-default</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <modules>
                <module>modules</module>
            </modules>
        </profile>

        <!-- Empty profile without modules. This profile should be used together with the antrun:run goal in order to run the ant deploy target -->
        <profile>
            <id>profile-empty</id>
            <modules/>
        </profile>
    </profiles>

