  <xsd:complexType name="CT_Kinsoku">
    <xsd:attribute name="lang" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation>Language</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
