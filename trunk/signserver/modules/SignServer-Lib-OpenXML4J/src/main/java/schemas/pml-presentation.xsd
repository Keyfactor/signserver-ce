      <xsd:enumeration value="letter">
        <xsd:annotation>
          <xsd:documentation>Letter</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="A4">
        <xsd:annotation>
          <xsd:documentation>A4</xsd:documentation>
