    <xsd:restriction base="a:ST_PositiveCoordinate32">
      <xsd:minInclusive value="914400" />
      <xsd:maxInclusive value="51206400" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ST_SlideSizeType">
    <xsd:annotation>
      <xsd:documentation>Slide Size Type</xsd:documentation>
