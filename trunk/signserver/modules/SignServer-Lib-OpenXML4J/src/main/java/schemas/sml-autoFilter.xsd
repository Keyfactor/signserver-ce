    <xsd:choice minOccurs="0" maxOccurs="1">
      <xsd:element name="filters" type="CT_Filters" minOccurs="0" maxOccurs="1">
        <xsd:annotation>
          <xsd:documentation>Filter Criteria</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
