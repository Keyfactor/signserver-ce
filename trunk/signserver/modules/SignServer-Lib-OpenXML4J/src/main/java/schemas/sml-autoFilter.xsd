        <xsd:annotation>
          <xsd:documentation>Icon Filter</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="extLst" type="CT_ExtensionList" minOccurs="0" maxOccurs="1"></xsd:element>
